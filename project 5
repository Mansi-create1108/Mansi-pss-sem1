#include <stdio.h>
void main()
{
char A;
printf("Enter any character");
scanf("%c",&A);

if(A=='a'||A=='e'||A=='i'||A=='o'||A=='u'||A=='A'||A=='E'||A=='I'||A=='O'||A=='U')
{
printf("%c is vowel",A);}
else if ((A>='a' && A<='z')||(A>='A' && A<='Z'))
{
printf("%c is consonant",A);
}
else{printf("wrong input");}
}
