# include <stdio.h>
void main()
{
int a,b,c;
printf("Enter three sides of a triangle");
scanf("%d",&a);
scanf("%d",&b);
scanf("%d",&c);
if(a==b && a==c)
{printf("it is a equilateral triangle");}
else if (a==b || a==c || b==c)
{printf("it is a isosceles triangle");}
else
{printf("it is a scalen triangle");}
}
